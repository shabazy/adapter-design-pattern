{
  "name": "gokhanensep/adapter-design-pattern",
  "description": "adapter-design-pattern",
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.1",
    "guzzlehttp/guzzle": "^6.3"
  },
  "autoload": {
    "psr-4": { "DesignPattern\\": "src/" },
    "classmap": ["src/"]
  },
  "autoload-dev": {
    "psr-4": {
      "DesignPatternTest\\": "src/tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3"
  }
}
